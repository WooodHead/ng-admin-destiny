{
  "manifest_version": 2,

  "name": "ng-admin-destiny",
  "description": "A responsive layout chrome extension for Desinty",
  "version": "0.0.5",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "short_name": "nad",
  "background": {
    "scripts": [
      "tab.js"
    ]
  },
  "browser_action": {
      "default_icon": {
      		"19": "icon.png",
      		"38": "icon38.png"
      	}
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "storage",
    "*://*.bungie.net/*",
    "cookies"
  ]
}